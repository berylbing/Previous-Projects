package com.ht706.componentLibrary.componentFeedBack.actionForm;

import java.util.ArrayList;
import java.util.Date;
import java.util.HashSet;
import java.util.List;
import java.util.Set;

import org.apache.struts.action.ActionForm;
import org.apache.struts.upload.FormFile;

import com.ht706.componentLibrary.componentFeedBack.bean.hbm.ProblemReport;
import com.ht706.componentLibrary.componentRelease.bean.hbm.Asset;


public class ProblemReportForm extends ActionForm {

	private String rid;
	private Asset asset;
	private String reportId;
	private String reporter;
	private String dept;
	private String project;
	private String background;
	private String description;
	private String suggestion;
	private Integer harm;
	private String discoveryDate;
	private Date reportDate;
	private FormFile file;
	private String status;
	private Set problemtraces = new HashSet(0);
	
	private List<ProblemReport> problembyme = new ArrayList<ProblemReport>();
	
	public String getRid() {
		return rid;
	}
	public void setRid(String rid) {
		this.rid = rid;
	}
	public Asset getAsset() {
		return asset;
	}
	public void setAsset(Asset asset) {
		this.asset = asset;
	}
	public String getReportId() {
		return reportId;
	}
	public void setReportId(String reportId) {
		this.reportId = reportId;
	}
	public String getReporter() {
		return reporter;
	}
	public void setReporter(String reporter) {
		this.reporter = reporter;
	}
	public String getDept() {
		return dept;
	}
	public void setDept(String dept) {
		this.dept = dept;
	}
	public String getProject() {
		return project;
	}
	public void setProject(String project) {
		this.project = project;
	}
	public String getBackground() {
		return background;
	}
	public void setBackground(String background) {
		this.background = background;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public String getSuggestion() {
		return suggestion;
	}
	public void setSuggestion(String suggestion) {
		this.suggestion = suggestion;
	}
	public Integer getHarm() {
		return harm;
	}
	public void setHarm(Integer harm) {
		this.harm = harm;
	}

	public String getDiscoveryDate() {
		return discoveryDate;
	}
	public void setDiscoveryDate(String discoveryDate) {
		this.discoveryDate = discoveryDate;
	}
	public Date getReportDate() {
		return reportDate;
	}
	public void setReportDate(Date reportDate) {
		this.reportDate = reportDate;
	}

	public FormFile getFile() {
		return file;
	}
	public void setFile(FormFile file) {
		this.file = file;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public Set getProblemtraces() {
		return problemtraces;
	}
	public void setProblemtraces(Set problemtraces) {
		this.problemtraces = problemtraces;
	}
	public void setProblembyme(List<ProblemReport> problembyme) {
		this.problembyme = problembyme;
	}
	public List<ProblemReport> getProblembyme() {
		return problembyme;
	}
	
	
}
