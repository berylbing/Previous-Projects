<%@ page contentType="text/html; charset=gbk" %>
<%@ taglib uri="/tags/struts-bean" prefix="bean" %>
<%@ taglib uri="/tags/struts-logic" prefix="logic" %>
<%@ taglib uri="/tags/struts-html" prefix="html" %>
<%@ taglib uri="/tags/struts-nested" prefix="nested" %>
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<title>Untitled Document</title>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312">
<link href="../css/common.css" rel="stylesheet" type="text/css">
<script src="js/check.js" type="text/javascript"></script>

<body>

<div class=list>
  <table border="0" id="ta">
    <tr>      
      <th align="center" colspan="3">信息统计</th>    
    </tr>
    <tr>
      <td>用户总人数</td>    
      <td>${userNumber}</td>
      <td align="right"><a href="ReportAction.do?method=listUser">查看明细&nbsp;&nbsp;&nbsp;</a></td>         
    </tr>
    <tr>
      <td>构件总个数</td>
      <td>${assetNumber}</td>  
      <td align="right"><a href="ReportAction.do?method=listAsset">查看明细&nbsp;&nbsp;&nbsp;</a></td>        
    </tr>
    <tr>
      <td>规约变更次数</td>
      <td>${specificationmodifyNumber}</td> 
      <td align="right"><a href="ReportAction.do?method=listSpecificationModify">查看明细&nbsp;&nbsp;&nbsp;</a></td>           
    </tr>    
    <tr>
      <td>出库总次数</td>
      <td>${checkoutNumber}</td>  
      <td align="right"><a href="ReportAction.do?method=listCheckOut">查看明细&nbsp;&nbsp;&nbsp;</a></td>        
    </tr>
    <tr>
      <td>检索总次数</td>
      <td>${activitystatisticNumber }</td> 
      <td align="right"><a href="ReportAction.do?method=listActivityStatic">查看明细&nbsp;&nbsp;&nbsp;</a></td>           
    </tr> 
    <tr>
      <td>问题报告总数</td>
      <td>${problemreportNumber}</td> 
      <td align="right"><a href="ReportAction.do?method=listProblemReport">查看明细&nbsp;&nbsp;&nbsp;</a></td>           
    </tr>
    <tr>
      <td>反馈总次数</td>
      <td>${feedbackNumber}</td> 
      <td align="right"><a href="ReportAction.do?method=listfeedback">查看明细&nbsp;&nbsp;&nbsp;</a></td>           
    </tr>
    <tr>
      <td>刻面总数</td>
      <td>${facetNumber}</td> 
      <td align="right"><a href="ReportAction.do?method=listFacet">查看明细&nbsp;&nbsp;&nbsp;</a></td>           
    </tr> 
     <tr>
      <td>术语总数</td>
      <td>${wordNumber}</td> 
      <td align="right"><a href="ReportAction.do?method=listWord">查看明细&nbsp;&nbsp;&nbsp;</a></td>           
    </tr>  
     <tr>
      <td>已定义的关系类型个数</td>
      <td>${relationNumber}</td> 
      <td align="right"><a href="ReportAction.do?method=listRelation">查看明细&nbsp;&nbsp;&nbsp;</a></td>           
    </tr>                       
 </table>
</div>

</body>
</html>