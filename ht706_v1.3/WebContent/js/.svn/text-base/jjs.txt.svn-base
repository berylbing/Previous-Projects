<script language="javascript">
	function getURL(url){
		var xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
		xmlhttp.open("GET",url,false);
		xmlhttp.send();
		if (xmlhttp.readyState==4) return xmlhttp.Status==200;
			return false;
	}
	function mm(e, url){
		if(!getURL(e.href)) e.href = url;
	}
</script>

