//取得控件得绝对位置(1) 
function getoffset(e) {  
	var t=e.offsetTop;  
	var l=e.offsetLeft;  
	while(e=e.offsetParent) {  
		t+=e.offsetTop;  
		l+=e.offsetLeft;  
	}  
	var rec = new Array(1); 
	rec[0]  = t; 
	rec[1] = l; 
	return rec 
}  

