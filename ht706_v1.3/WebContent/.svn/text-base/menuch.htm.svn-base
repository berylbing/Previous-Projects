<script>
	
	function delm(v) {
		setTimeout(del,400);
		function del(){
			var s = v.all.tags("div")[0];
			if (s != null)
				s.removeNode(true);
		}
	}
	
	function getPos(elmt) { // 得到座标
		var pos = new Array();
		
		var t = elmt.offsetTop;
		var l = elmt.offsetLeft;
		while (elmt = elmt.offsetParent) {
			t += elmt.offsetTop;
			l += elmt.offsetLeft;
		}
		
		pos[0] = t;
		pos[1] = l;
		
		return pos;
	}
</script>
