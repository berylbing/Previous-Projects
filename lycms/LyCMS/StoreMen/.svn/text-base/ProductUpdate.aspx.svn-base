<%@ Page Title="" Language="C#" MasterPageFile="~/Frame/MainLeftStore.master" AutoEventWireup="true" CodeFile="ProductUpdate.aspx.cs" Inherits="StoreMen_ProductUpdate" %>

<asp:Content ID="Content1" ContentPlaceHolderID="ContentPlaceHolder2" Runat="Server">
<div class="right_content" cellpadding="0" cellspacing="0" style="height:480px; width:815px; background-color:#F0F0F0;">
       <table>
          <tr style="height:50px;">
          </tr>
          <tr align="center">
    
           <td align="center" style=" font-size:20px">
            产品修改页面
           </td>
         </tr>
        </table>
        
      <table width="98%"  border="0" cellpadding="0" cellspacing="0" class="text008">
        <tr align="center" >
          <td height="50" align="right" style="font-size:15px; width: 130px;">产品名称</td>
          <td align="left" style="width:100px">
              <asp:TextBox ID="ProductName" runat="server" BorderColor="#7197F0" Font-Size="15px" Height="20px" Width="150px"></asp:TextBox>
          </td>
          <td height="50" align="right"style="font-size:15px; width: 130px;" >产品总分</td>
          <td align="left">
              <asp:TextBox ID="ProductType1"  runat="server" BorderColor="#7197F0" Font-Size="15px" Height="20px" Width="150px"></asp:TextBox>
          </td>
        </tr>
        <tr align="center">
          <td height="50" align="right" style="font-size:15px; width: 130px;">分类</td>
          <td align="left">
              <asp:TextBox ID="ProductType2" runat="server" CssClass="inp001" BorderColor="#7197F0" Font-Size="15px" Height="20px" Width="150px"></asp:TextBox>
          </td>
          <td height="50" align="right" style="font-size:15px; width: 130px;">明细</td>
          <td align="left">
              <asp:TextBox ID="ProductType3" runat="server"  BorderColor="#7197F0" Font-Size="15px" Height="20px" Width="150px"></asp:TextBox>
          </td>
        </tr>
          <tr align="center">
              <td height="50" align="right" style="font-size:15px; width: 130px;" >
                 产品规格
              </td>
              <td align="left">
                  <asp:TextBox ID="ProductStandard" runat="server" BorderColor="#7197F0" Font-Size="15px" Height="20px" Width="150px"></asp:TextBox>
              </td>
              <td height="50" align="right" style="font-size:15px; width: 130px;">
                 批量
              </td>
              <td align="left">
                  <asp:TextBox ID="ProductAmount" runat="server"  BorderColor="#7197F0" Font-Size="15px" Height="20px" Width="150px"></asp:TextBox>
              </td>
          </tr>
          <tr align="center">
              <td height="50" align="right" style="font-size:15px; width: 130px;">
                 预警上限
              </td>
              <td align="left">
                 <asp:TextBox ID="ProductUp" runat="server"  BorderColor="#7197F0" Font-Size="15px" Height="20px" Width="150px"></asp:TextBox>
              </td>
               <td height="50" align="right" style="font-size:15px; width: 130px;">
                 预警下限
              </td>
              <td align="left">
                 <asp:TextBox ID="ProductDown" runat="server" BorderColor="#7197F0"  Font-Size="15px" Height="20px" Width="150px"></asp:TextBox>
              </td>
          </tr>
          <tr align="center">
              <td height="50" align="right" style="font-size:15px; width: 130px;">
                单位
              </td>
              <td align="left">
                <asp:TextBox ID="ProductUnit" runat="server" BorderColor="#7197F0" Font-Size="15px" Height="20px" Width="150px"></asp:TextBox>
              </td>
              <td height="50" align="right" style="font-size:15px; width: 130px;">
                库存量 
              </td>
              <td align="left">
                 <asp:TextBox ID="ProductStorage" runat="server" BorderColor="#7197F0" Font-Size="15px" Height="20px" Width="150px"></asp:TextBox>
              </td>
            <tr>
              <td height="50" align="right" style="font-size:15px; width: 130px;">
                详细信息
              </td>
              <td align="left">
                <asp:TextBox ID="ProductDetail" runat="server" BorderColor="#7197F0" Font-Size="15px" Height="20px" Width="150px" ></asp:TextBox>
              </td>
            </tr>
          </tr>
       </table>
    
    
    <table width="98%">
     <tr>
      <td style="width:50%;" align="left">
      </td>
      <td align="right">
        <asp:Button ID="Button1"  Text="确定"  runat="server"  Width="72px" Height="34px" />
     
      </td>
      <td style="width:100px">
      </td>
      <td align="left">
        <asp:Button ID="Button2" Text="取消" runat="server" Width="72px" Height="34px" />
      </td>
     </tr>
    </table>
    
    </div>
</asp:Content>

